<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smu.sumubee">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name="com.smu.sumubee.MasterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SUMUBI"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.smu.sumubee.AssignActivity"
            android:exported="false" />
        <activity
            android:name="com.smu.sumubee.NoticeActivity"
            android:exported="false" />
        <activity
            android:name="com.smu.sumubee.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.smu.sumubee.BoardActivity" />
        <activity android:name="com.smu.sumubee.MainActivity" />
        <activity android:name="com.smu.sumubee.UpdateActivity" />
        <activity android:name="com.smu.sumubee.CreateActivity" />
        <activity android:name="com.smu.sumubee.UserInfoActivity" />
        <activity android:name="com.smu.sumubee.LoginActivity" />
        <activity android:name="com.smu.sumubee.PlannerActivity" />
        <activity android:name="com.smu.sumubee.SubjectActivity" />
    </application>

</manifest>